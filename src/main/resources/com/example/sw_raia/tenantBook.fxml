<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sw_raia.TenantControll">
    <Label layoutX="14.0" layoutY="5.0" text="Select a Housing Unit to Book:" />
    <TextField fx:id="selectedHouseTextField" layoutX="14.0" layoutY="32.0" prefWidth="200.0" />
    <Label layoutX="14.0" layoutY="104.0" text="Move-in Date:" />
    <DatePicker fx:id="moveInDatePicker" layoutX="120.0" layoutY="100.0" />
    <Label layoutX="13.0" layoutY="150.0" text="Lease Duration (months):" />
    <TextField fx:id="leaseDurationTextField" layoutX="150.0" layoutY="146.0" />
    <!-- Add a button to submit the booking request -->
    <Button fx:id="bookButton" layoutX="141.0" layoutY="200.0" text="Book" />
</AnchorPane>
